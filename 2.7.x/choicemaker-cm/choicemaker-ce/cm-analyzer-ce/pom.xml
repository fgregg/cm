  <name>CM Analyzer, Community Edition</name>
  <description><![CDATA[
  Zip distribution of CM Analyzer, Community Edition.
  ]]>
  </description>

  <properties>
    <eclipse.application.dir>${project.build.directory}/eclipse.application.dir</eclipse.application.dir>
    <eclipse.application.plugins.dir>${eclipse.application.dir}/plugins</eclipse.application.plugins.dir>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <executions>
          <execution>
            <id>echo-params</id>
            <phase>validate</phase>
            <configuration>
              <target>
                <echo message="eclipse.application.dir: ${eclipse.application.dir}" />
                <echo
                  message="eclipse.application.plugins.dir: ${eclipse.application.plugins.dir}" />
                <echo
                  message="project.build.scriptSourceDirectory: ${project.build.scriptSourceDirectory}" />
              </target>
            </configuration>
            <goals>
              <goal>run</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <executions>
          <execution>
            <id>plugins</id>
            <phase>package</phase>
            <goals>
              <goal>unpack-dependencies</goal>
            </goals>
            <configuration>
              <excludeTransitive>true</excludeTransitive>
              <outputDirectory>${eclipse.application.plugins.dir}</outputDirectory>
              <includeClassifiers>eclipse2prj</includeClassifiers>
            </configuration>
          </execution>
          <execution>
            <id>startup.jar</id>
            <phase>package</phase>
            <goals>
              <goal>copy</goal>
            </goals>
            <configuration>
              <artifactItems>
                <artifactItem>
                  <groupId>com.choicemaker.cm</groupId>
                  <artifactId>org.eclipse.core.launcher</artifactId>
                  <version>2.1.3</version>
                  <outputDirectory>
                    ${eclipse.application.dir}</outputDirectory>
                  <destFileName>startup.jar</destFileName>
                </artifactItem>
              </artifactItems>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-assembly-plugin</artifactId>
        <executions>
          <execution>
            <id>assembly</id>
            <phase>package</phase>
            <configuration>
              <descriptor>src/main/assembly/assembly.xml</descriptor>
            </configuration>
            <goals>
              <goal>single</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

